
class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        if(head==NULL || head->next==NULL)
        return NULL;
        
        ListNode* fast=head;
        ListNode* slow=head;
        
        while(n--){   
            fast=fast->next;
        }
        if(fast==NULL){
            return head->next;
        }
        while(fast!=NULL && fast->next!=NULL){
            fast=fast->next;
            slow=slow->next;
        }
        slow->next=slow->next->next;
        return head;
    }
};